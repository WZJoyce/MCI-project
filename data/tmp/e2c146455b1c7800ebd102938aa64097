#include <iostream>

int main(int argc, const char * argv[]) {

    size_t j = 0b0100'1100'0110;
    int mark = 0;
    if (j == 0b0100'1100'0110)
    {
    	std::cout << "Correct!!" << std::endl;
    	mark = 100;
    }
    std::cout << "submission mark:" << std::endl;
    std::cout << mark << std::endl;
    return 0;
}
